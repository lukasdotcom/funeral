{
  "name": "funeral",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dbtypes": "kysely-codegen --url ./test.db --out-file ./lib/types.ts",
    "dbtypes:check": "kysely-codegen --url ./test.db --out-file ./lib/types.ts --verify --log-level=error",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "migrate": "npx tsx ./lib/migrate.ts",
    "docker": "rm build && npm run docker-first-run; npm run start",
    "docker-first-run": "npm run migrate; npm run build"
  },
  "dependencies": {
    "@radix-ui/react-label": "2.1.2",
    "@radix-ui/react-slot": "1.1.2",
    "@tailwindcss/postcss": "4",
    "@types/better-sqlite3": "7.6.12",
    "better-sqlite3": "11.9.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "kysely": "0.27.6",
    "lucide-react": "0.482.0",
    "next": "15.2.2",
    "next-themes": "0.4.6",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "tailwind-merge": "3.0.2",
    "tailwindcss-animate": "1.0.7",
    "ts-node": "10.9.2",
    "tsx": "4.19.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3",
    "@types/node": "24",
    "@types/react": "19",
    "@types/react-dom": "19",
    "eslint": "9",
    "eslint-config-next": "15.2.2",
    "kysely-codegen": "0.18.3",
    "prettier": "3.7.4",
    "tailwindcss": "4",
    "typescript": "5"
  }
}
